textures/dirt/dirt_a
{
	qer_editorimage textures/dirt/dirt_a.tga
  q3map_nonplanar
  q3map_shadeangle 179
  q3map_tcGen ivector ( 512 0 0 ) ( 0 512 0 )
	{
		map textures/dirt/dirt_a.tga
    tcMod scale 2 2
    rgbGen vertex
	}
	{
    map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}
